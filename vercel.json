{
  "version": 2,
  "functions": {
    "api/index.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/admin",
      "destination": "/server/public/admin.html"
    },
    {
      "source": "/api/admin/users",
      "destination": "/api/index.js?url=/api/admin/users"
    },
    {
      "source": "/api/check-user",
      "destination": "/api/index.js?url=/api/check-user"
    },
    {
      "source": "/api/submit",
      "destination": "/api/index.js?url=/api/submit"
    },
    {
      "source": "/api/admin/user/(.*)",
      "destination": "/api/index.js?url=/api/admin/user/$1"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js?url=/api/$1"
    }
  ]
}
