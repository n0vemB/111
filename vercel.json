{
  "version": 2,
  "rewrites": [
    {
      "source": "/admin",
      "destination": "/api/index?url=/api/admin"
    },
    {
      "source": "/api/admin/users",
      "destination": "/api/index?url=/api/admin/users"
    },
    {
      "source": "/api/check-user",
      "destination": "/api/index?url=/api/check-user"
    },
    {
      "source": "/api/submit",
      "destination": "/api/index?url=/api/submit"
    },
    {
      "source": "/api/admin/user/(.*)",
      "destination": "/api/index?url=/api/admin/user/$1"
    }
  ]
}
